<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
        PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap>
    <typeAlias alias="UserDO" type="com.king.program.pojo.UserDO"/>

    <insert id="insert_user" parameterClass="UserDO">
        <![CDATA[
            insert into attend(username, password) values(#username#, #password#)
        ]]>
    </insert>

    <select id="select_password" parameterClass="String" resultMap="String">
        <![CDATA[
            select password from user where username = #username#
        ]]>
    </select>
</sqlMap>